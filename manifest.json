{
    "manifest_version": 3,
    "name": "SN Redirect to login from SSO",
    "description": "When being taken to MS SSO from SN instance URL, instead redirect to instance login.do page",
    "version": "1.0",

    "background": {
        "service_worker": "background.js"
    },

    "permissions": ["tabs" ,"storage"],

    "action": {
        "default_icon": {
            "16": "/images/icon16.png",
            "32": "/images/icon32.png",
            "48": "/images/icon48.png",
            "128": "/images/icon128.png"
        }
    }
}